using System; //unity sys loader

public class Character : Monobehaviour //monobehaviour required for code. character should be connector
{

    public Rigidbody2D rb;
    
    public float Speed = 5f; //spd variable
    private bool isJumpPressed = false;
    private bool isMoving = False;
    
    void Start()
    {
        rb = GetComponenet<Rigidbody2D>();
    }
    void Update()
    {
        // Vector3 move = new Vector3(Input.GetAxis("Horizontal"),0,Input.GetAxis("Vertical"));
        isJumpPressed = Input.GetButtonDown("Jump");
        characterController.Move(move * Time.deltatime * Speed)
    }
    void FixedUpdate()
    {
        if (isJumpPressed)
        {
            rb.velocity = new Vector3(0, 10, 0)
            isMoving = true;
            Debug.log("jump");
        }
        if (isMoving)
        {
            time = time + Time.fixedDeltaTime;             
            if ( time > 10.0f)
            {
                Debug.log(gameObject.transform.position.y + " : " + time);    
                time = 0.0f;
}
